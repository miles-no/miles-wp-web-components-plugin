{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "miles-blocks/miles-contact-card",
  "version": "0.1.0",
  "title": "Miles Contact Card Block",
  "category": "widgets",
  "icon": "index-card",
  "description": "Contact information block",
  "supports": {
    "html": false
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "orgnr": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "image": {
      "type": "string"
    }
  },
  "textdomain": "miles-contact-card",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}