{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "miles-blocks/miles-info-block",
  "version": "0.1.0",
  "title": "Miles info block",
  "category": "widgets",
  "icon": "text",
  "description": "Info block with heading and text circumvented by a rounded border.",
  "supports": {
    "html": false
  },
  "attributes": {
    "id": {
      "type": "string"
    },
    "heading": {
      "type": "string"
    },
    "text": {
      "type": "string"
    }
  },
  "textdomain": "miles-info-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}